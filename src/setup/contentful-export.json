      "displayField": "text",
      "name": "Microblog",
      "description": "A daily post of something fun!",
      "fields": [
        {
          "id": "text",
          "name": "Text",
          "type": "Text",
          "localized": false,
          "required": false,
          "validations": [
            {
              "size": {
                "max": 280
              },
              "message": "Write a shorter post, yo! (Fewer than 280 characters)"
            }
          ],
          "disabled": false,
          "omitted": false
        },
        {
          "id": "image",
          "name": "Image",
          "type": "Link",
          "localized": false,
          "required": false,
          "validations": [
            {
              "linkMimetypeGroup": [
                "image"
              ],
              "message": "Please choose an image file, yo!"
            }
          ],
          "disabled": false,
          "omitted": false,
          "linkType": "Asset"
        },
        {
          "id": "link",
          "name": "Link",
          "type": "Symbol",
          "localized": false,
          "required": false,
          "validations": [
            {
              "regexp": {
                "pattern": "^(ftp|http|https):\\/\\/(\\w+:{0,1}\\w*@)?(\\S+)(:[0-9]+)?(\\/|\\/([\\w#!:.?+=&%@!\\-/]))?$",
                "flags": null
              },
              "message": "Make sure your link is formatted properly, yo!"
            }
          ],
          "disabled": false,
          "omitted": false
        },
        {
          "id": "linkText",
          "name": "Link text",
          "type": "Text",
          "localized": false,
          "required": false,
          "validations": [
            {
              "size": {
                "max": 100
              },
              "message": "Please enter shorter link text, yo! (Fewer than 100 characters)"
            }
          ],
          "disabled": false,
          "omitted": false
        },
        {
          "id": "panther",
